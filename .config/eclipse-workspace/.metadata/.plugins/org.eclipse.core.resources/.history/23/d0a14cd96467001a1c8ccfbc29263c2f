
public class Main {

	public static void main(String[] args) {
		System.out.println("Hello World");
		
		Person bob = null;
		System.out.println(bob);
		bob = new Person();
		System.out.println(bob);
		
		bob.name = "bob";
		bob.greet();
		
		Person jane = bob;
		jane.name = "Jane";
		jane.greet();
		
		bob.greet();
 }

}
